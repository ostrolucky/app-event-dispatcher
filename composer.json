{
  "name": "ostrolucky/app-event-dispatcher",
  "description": "Event Dispatcher for your application domain",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel OstroluckÃ½",
      "email": "gabriel.ostrolucky@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": { "Ostrolucky\\AppEventDispatcher\\": "src" }
  },
  "autoload-dev": {
    "psr-4": { "Ostrolucky\\AppEventDispatcher\\Test\\": "test" }
  },
  "require-dev": {
    "doctrine/orm": "*",
    "elliotchance/concise": "^2.1",
    "infection/infection": "^0.7.0",
    "phpunit/phpunit": "^5",
    "symfony/dependency-injection": "*",
    "symfony/event-dispatcher": "*",
    "symfony/workflow": "*"
  },
  "suggest": {
    "symfony/dependency-injection": "To automatically attach your services to event dispatcher based on service tags"
  },
  "config": {
    "sort-packages": true
  }
}
